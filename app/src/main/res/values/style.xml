<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextViewBoyCotStyle">
        <item name="android:textSize">@dimen/_30ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:background">@drawable/edit_text_background</item>
        <item name="android:inputType">number</item>
    </style>
    <style name="TextButtonStyle">
        <item name="android:textSize">@dimen/_19ssp</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/text_button_background</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="CardHomeStyle">
        <item name="cardMaxElevation">@dimen/_5sdp</item>
        <item name="cardUseCompatPadding">true</item>
        <item name="cardCornerRadius">@dimen/_5sdp</item>
        <item name="cardBackgroundColor">@color/red</item>
    </style>
</resources>